{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-fabric-test"
		},
		"ls_dl_fabrictest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dl_fabrictest'"
		},
		"ls_st_fabrictest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_st_fabrictest'"
		},
		"ls_sql_fabrictest_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_fabrictest'"
		},
		"ls_dl_fabrictest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlfabrictest0001.dfs.core.windows.net/"
		},
		"ls_sql_fabrictest_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver-fabrictest.database.windows.net"
		},
		"ls_sql_fabrictest_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-fabrictest"
		},
		"ls_sql_fabrictest_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "fabrictest"
		},
		"trigger_tw_properties_pipeline_parameters_pEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"trigger_tw_properties_pipeline_parameters_pStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-fabric-test_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_dl_fabrictest_accountKey": {
						"value": "[parameters('ls_dl_fabrictest_accountKey')]"
					},
					"ls_st_fabrictest_connectionString": {
						"value": "[parameters('ls_st_fabrictest_connectionString')]"
					},
					"ls_sql_fabrictest_password": {
						"value": "[parameters('ls_sql_fabrictest_password')]"
					},
					"ls_dl_fabrictest_properties_typeProperties_url": {
						"value": "[parameters('ls_dl_fabrictest_properties_typeProperties_url')]"
					},
					"ls_sql_fabrictest_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_fabrictest_properties_typeProperties_server')]"
					},
					"ls_sql_fabrictest_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_fabrictest_properties_typeProperties_database')]"
					},
					"ls_sql_fabrictest_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_fabrictest_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-fabric-test_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-fabric-test_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-fabric-test_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-fabric-test_ArmTemplate_1')]"
			]
		},
		{
			"name": "adf-fabric-test_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger_tw_properties_pipeline_parameters_pEnd": {
						"value": "[parameters('trigger_tw_properties_pipeline_parameters_pEnd')]"
					},
					"trigger_tw_properties_pipeline_parameters_pStart": {
						"value": "[parameters('trigger_tw_properties_pipeline_parameters_pStart')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-fabric-test_ArmTemplate_2')]"
			]
		},
		{
			"name": "adf-fabric-test_ArmTemplate_4",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_4.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-fabric-test_ArmTemplate_3')]"
			]
		}
	]
}